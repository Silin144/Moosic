{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/",
      "dest": "/dist/index.html"
    }
  ],
  "env": {
    "FRONTEND_URL": "https://moosic-liart.vercel.app",
    "SPOTIFY_REDIRECT_URI": "https://moosic-liart.vercel.app/api/callback"
  }
}