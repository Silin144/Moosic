{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python@3.1.0"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build@1.0.0",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}