{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.py"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "^/(?!api).*",
      "dest": "/dist/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}