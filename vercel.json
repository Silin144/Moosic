{
  "version": 2,
  "buildCommand": "./build.sh",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "FRONTEND_URL": "https://moosic-liart.vercel.app",
    "SPOTIFY_REDIRECT_URI": "https://moosic-liart.vercel.app/api/callback"
  }
}